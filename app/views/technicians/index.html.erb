<p id="notice"><%= notice %></p>
<div class="container">
<h1>Technicians</h1>

<div class="table-responsive">
  <table class="table table-hover">
  <thead>
    <tr>
      <th>Tech name</th>
      <th>Technician status</th>
      <th>Tech expertise notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @technicians.each do |technician| %>
      <tr>
        <td><%= technician.tech_name %></td>
        <td><%= technician.technician_status %></td>
        <td><%= technician.tech_expertise_notes %></td>
        <td><%= link_to 'Show', technician %></td>
        <td><%= link_to 'Edit', edit_technician_path(technician) %></td>
        <td><%= link_to 'Destroy', technician, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
<%= link_to 'New Technician', new_technician_path %>
</div>