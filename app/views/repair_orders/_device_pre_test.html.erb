<fieldset>
<table>
  <tr>
    <td>
      <div class="icon">
        <%= form.hidden_field :_destroy, as: :hidden %>
        <%= link_to image_tag("https://i.imgur.com/xJea3q2.png"), '#', class: 'remove_record' %>
      </div>
     </td>
    <td>
      <div class="field">
        <%= form.label :test_id %>
        <%= form.collection_select :test_id, Test.all, :id, :test_name %>
      </div>
    </td>
    <td>
      <div class="field">
        <%= form.label :pre_test_condition, "Pre-Test Condition" %>
        <%= form.check_box :pre_test_condition, id: :device_pre_test_pre_test_condition %>
      </div>
    </td>
  </tr>
</table>
</fieldset>