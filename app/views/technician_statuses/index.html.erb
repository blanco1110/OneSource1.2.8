<p id="notice"><%= notice %></p>
<div class="container">
<h1>Technician Statuses</h1>
  <table class="table table-hover table-responsive">
  <thead>
    <tr>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @technician_statuses.each do |technician_status| %>
      <tr>
        <td><%= technician_status.tech_status %></td>
        <td><%= link_to 'Show', technician_status %></td>
        <td><%= link_to 'Edit', edit_technician_status_path(technician_status) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

    <div class="col-sm-1 col-xs-1 col-md-1 col-lg-1">
      <a class="btn btn-outline" href="technician_statuses/new">New Technician Status</a>
    </div>
</div>