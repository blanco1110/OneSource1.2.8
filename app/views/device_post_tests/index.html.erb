<p id="notice"><%= notice %></p>

<h1>Device Post Tests</h1>

<table>
  <thead>
    <tr>
      <th>Device</th>
      <th>Test</th>
      <th>Post Test Condition</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @device_post_tests.each do |device_post_test| %>
      <tr>
        <td><%= device_post_test.device %></td>
        <td><%= device_post_test.test %></td>
        <td><%= device_post_test.post_test_condition %></td>
        <td><%= link_to 'Show', device_post_test %></td>
        <td><%= link_to 'Edit', edit_device_post_test_path(device_post_test) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Device Post Test', new_device_post_test_path %>
