<p id="notice"><%= notice %></p>
<div class="container">
<h1>Device Component Statuses</h1>
  <table class="table table-hover table-responsive">
  <thead>
    <tr>
      <th>Device Component Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @device_component_statuses.each do |device_component_status| %>
      <tr>
        <td><%= device_component_status.device_comp_status %></td>
        <td><%= link_to 'Show', device_component_status %></td>
        <td><%= link_to 'Edit', edit_device_component_status_path(device_component_status) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
  <div class="col-sm-1 col-xs-1 col-md-1 col-lg-1">
  <a class="btn btn-outline" href="device_component_statuses/new">New Device Component Status</a>
</div>
</div>